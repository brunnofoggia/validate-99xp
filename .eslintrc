{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "_": true,
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "array-bracket-spacing": [ 2, "never" ],
    "block-scoped-var": 2,
    "brace-style": [ 2, "1tbs", { "allowSingleLine": true } ],
    "camelcase": [ 2, { "properties": "always" } ],
    "curly": 2,
    "dot-notation": [ 2, { "allowKeywords": true } ],
    "eol-last": 2,
    "eqeqeq": [ 2, "allow-null" ],
    "guard-for-in": 0,
    "indent": 0,
    "key-spacing": [ 2,
    {
        "beforeColon": false,
        "afterColon": true
    }
    ],
    "keyword-spacing": [ 2 ],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-spaces": "error",
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-new": 2,
    "no-proto": 0,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 0,
    "no-undef": 0,
    "no-unused-vars": [ 1, { "args": "none" } ],
    "no-useless-escape": 0,
    "no-var": 0,
    "no-with": 2,
    "object-shorthand": [ 2, "methods" ],
    "operator-linebreak": [ 2, "after" ],
    "quotes": [ 2, "single" ],
    "semi": [ 0, "never" ],
    "space-before-blocks": 0,
    "space-before-function-paren": 0,
    "space-in-parens": [ 2, "never" ],
    "space-infix-ops": 2,
    "space-unary-ops": [ 2,
    {
        "nonwords": false,
        "overrides": {}
    }
    ],
    "strict": 0,
    "valid-jsdoc": 0,
    "wrap-iife": [ 2, "inside" ]
  }
}
